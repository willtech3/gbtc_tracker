  jQuery(document).ready(function($) {
    jQuery("#modal-87").iziModal({
        padding: 15,
        overlay: true,
        overlayColor: 'rgba(0, 0, 0, 0.4)',
        width:992,
    });
    jQuery(document).on('click', '.modal-form87', function (event) {
        event.preventDefault();
        jQuery('#modal-87').iziModal('open');
    });
  });
</script>
