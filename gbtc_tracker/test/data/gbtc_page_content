        var hash = window.location.hash;
        if (hash) {
            if ("#overview" == hash) {
                jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(0)').trigger('click');
            } else if ("#market-performance" == hash) {
                jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(1)').trigger('click');
            } else if ("#resources" == hash) {
                jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(2)').trigger('click');
            }
        }
        jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(0)').click(function () {
            var page = 'Bitcoin Investment Trust';
            var url = '//grayscale.co/bitcoin-investment-trust/#overview';
            var obj = {Page: page, Url: url};
            history.pushState(obj, page, url);
        });
        jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(1)').click(function () {
            var page = 'Bitcoin Investment Trust';
            var url = '//grayscale.co/bitcoin-investment-trust/#market-performance';
            var obj = {Page: page, Url: url};
            history.pushState(obj, page, url);
        });
        jQuery('#skrollr-body > div.container > div > section.module.tabs > div.tabs-header > div:eq(2)').click(function () {
            var page = 'Bitcoin Investment Trust';
            var url = '//grayscale.co/bitcoin-investment-trust/#resources';
            var obj = {Page: page, Url: url};
            history.pushState(obj, page, url);
        });
    });
</script>
